    <Authors>dkzwm</Authors>
    <Description>C# version of Phonedata using binary search</Description>
    <PackageProjectUrl>https://github.com/dkzwm/PhoneDataLookup</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
